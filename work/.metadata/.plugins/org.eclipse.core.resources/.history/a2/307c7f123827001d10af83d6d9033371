package com.exe.springJdbcTemplate;



//create table custom
//(id number,
//name char(10),
//age number);


public class CustomDTO {
	
	private int id;
	private String name;
	private int age;
	
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}
